{
  "name": "Mizar",
  "version": "1.0.0",
  "description": "API of geographical and astronomical display and navigation",
  "main": "./build/generated/Mizar.min.js",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "jsdoc": "jsdoc -c jsdoc.conf",
    "jshint": "jshint -c ./.jshintrc  src/*",
