    "path": "^0.12.7",
    "proj4": "^2.3.15",
    "requirejs": "^2.3.2",
    "string": "^3.3.1",
    "uglify": "^0.1.5",
    "underscore": "^1.8.3",
    "unzip": "^0.1.11"
